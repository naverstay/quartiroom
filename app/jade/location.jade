include mixins.pug
doctype html
html(lang='ru')
    +dev_head("Quartiroom Компания", false, false, true)

    body
        .wrapper
            .base._gray
                +header(1)
                
                .section.breadcrumbs_section
                    .section-inner
                        +breadcrumbs(['Главная', 'Недвижимость в Королеве'])
                        
                        .section-title Недвижимость в Королеве
                            
                div.main_content
                    .section-inner
                        .location-block
                            .location-block-view_tabs
                                ul#location_switcher
                                    li.filter-item
                                        a.gl_link(href='#location_tab_1')
                                            span.mb.complex-view_tab-icon.i-humburger.mob_hidden
                                            span.mb фото
                                                span.mob_hidden (12)
                                    li.filter-item
                                        a.gl_link(href='#location_tab_2')
                                            span.mb.complex-view_tab-icon.i-map_pin.mob_hidden
                                            span.mb на карте
                            
                            .tabBlock(data-tab-context='#location_switcher')
                                #location_tab_1
                                    .location-block_slider-holder
                                        .location-block_gallery.locationGallery
                                            .location-block_slider.locationSlider
                                                +loop_while(1)
                                                    .slide
                                                        img(src='images/loc_1.jpg')
                                                        a.slide_zoom.btn_v1.btn_white_transp(href='#')
                                                            span.zoom_icon
                                                            span Увеличить
                                                    .slide
                                                        img(src='images/loc_2.jpg')
                                                        a.slide_zoom.btn_v1.btn_white_transp(href='#')
                                                            span.zoom_icon
                                                            span Увеличить
                                                    
                                            .location-block_slider-controls
                                                .slide_prev.btn_white_transp
                                                .slide_next.btn_white_transp
                                                
                                
                                #location_tab_2
                                    .location-block_map-holder
                                        .map-wrapper
                                            img(src='images/big_map.jpg')
                                            
                            .location-block_params
                                

                        .company-block.companyBlock.opened
                            .company-block_caption 
                                span Квартиры
                            .company-info_box
                                p 
                                    span._clr_gray-dark Сейчас у компании: 
                                    span 234 квартиры 
                                    span._clr_gray-dark и 
                                    span 15 таунхаусов
                                    
                                .company-unit_info_w
                                    dl.company-unit_info
